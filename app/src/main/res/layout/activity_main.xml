<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#43FEE970"
    tools:context=".view.MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="382dp"
        android:layout_height="592dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.24000001">

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Restaurant"
            android:textColor="#FF9800"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/btn_Burrito"
            android:layout_width="190dp"
            android:layout_height="266dp"
            android:layout_marginEnd="2dp"
            android:backgroundTint="#00FFFFFF"
            android:scaleType="fitCenter"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/btn_Sandwich"
            app:layout_constraintTop_toBottomOf="@+id/btn_HotDog"
            app:srcCompat="@drawable/buurrito" />

        <ImageButton
            android:id="@+id/btn_Sandwich"
            android:layout_width="190dp"
            android:layout_height="266dp"
            android:layout_marginBottom="4dp"
            android:backgroundTint="#00FFFFFF"
            android:scaleType="fitCenter"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/btn_Burrito"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/sandwicch" />

        <ImageButton
            android:id="@+id/btn_Burger"
            android:layout_width="190dp"
            android:layout_height="266dp"
            android:backgroundTint="#00FFFFFF"
            android:scaleType="fitCenter"
            app:layout_constraintBottom_toTopOf="@+id/btn_Sandwich"
            app:layout_constraintEnd_toStartOf="@+id/btn_HotDog"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/burger" />

        <ImageButton
            android:id="@+id/btn_HotDog"
            android:layout_width="190dp"
            android:layout_height="266dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="3dp"
            android:backgroundTint="#00FFFFFF"
            android:scaleType="fitCenter"
            app:layout_constraintBottom_toTopOf="@+id/btn_Burrito"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            app:srcCompat="@drawable/hotdogg" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>